{
	"name": "obsidian-gitsync",
	"version": "1.0.0",
	"description": "Sync your vault to GitHub. Works on mobile and desktop without Git installed.",
	"main": "main.js",
	"type": "module",
	"author": {
		"name": "Vishal Sharma",
		"email": "sharma39vishal@gmail.com",
		"url": "https://github.com/vishal-go"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/vishal-go/obsidian-gitsync"
	},
	"scripts": {
